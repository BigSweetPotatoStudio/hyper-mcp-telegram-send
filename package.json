    "koa-router": "^13.0.1",
    "markdown-it": "^14.1.0",
    "node-fetch-native": "^1.6.4",
    "node-telegram-bot-api": "^0.66.0"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa-router": "^7.4.8",
